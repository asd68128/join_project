<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sl.automobile.mapper.PermissionMapper">
    <sql id="PermissionSql">
        permissionId,
        permissionName,
        menu_name,
        menu_type,
        menu_url,
        per_desc,
        if_vilid,
        parent_id
    </sql>
    <select id="loadPermissionAll" resultType="com.sl.automobile.pojo.Permission">
        select
        *
        from
        permission
    </select>
</mapper>